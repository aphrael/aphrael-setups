
router: true

nise_bosh:
  ruby:
    prefix_path: '/usr/local/rbenv/versions/1.9.3-p429'
  deploy:
    jobs: [ 'router' ]
    manifest:
      jobs:
      - name: router
        template:
        - gorouter

      properties:
        networks:
          apps: default
          management: default
  
        nats:
          user: nats
          password: nats
          address: 127.0.0.1
          port: 4222
